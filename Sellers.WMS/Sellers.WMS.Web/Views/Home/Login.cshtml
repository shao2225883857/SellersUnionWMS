@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title></title>
    <link rel="shortcut icon" href="include/favicon.ico" type="image/x-icon" />
    <style>
        div, p, table, th, td, font
        {
            font-family: inherit;
            font-size: 1em;
            line-height: inherit;
        }

        div, blockquote, q, iframe, form, ul, ol, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, p
        {
            margin: 0;
            padding: 0;
        }

        input[type="text"], input[type="password"]
        {
            -moz-border-bottom-colors: none;
            -moz-border-left-colors: none;
            -moz-border-right-colors: none;
            -moz-border-top-colors: none;
            background: url("product/images/text_bg.gif") repeat-x scroll left top #F7FAFC;
            border-color: #99AABB #CCDDEE #CCDDEE #AABBCC;
            border-image: none;
            border-style: solid;
            border-width: 1px;
            color: #336699;
            padding: 3px 2px 2px;
            vertical-align: middle;
        }

        input.inputText
        {
            -moz-border-bottom-colors: none;
            -moz-border-left-colors: none;
            -moz-border-right-colors: none;
            -moz-border-top-colors: none;
            background: url("product/images/text_bg.gif") repeat-x scroll left top #F7FAFC;
            border-color: #99AABB #CCDDEE #CCDDEE #AABBCC;
            border-image: none;
            border-style: solid;
            border-width: 1px;
            color: #336699;
            padding: 3px 2px 2px;
            vertical-align: middle;
        }

        input
        {
            color: #114477;
            margin: 0;
            vertical-align: middle;
        }

        .loginPageBody
        {
            background: url("product/images/loginbodybg.gif") repeat-x scroll left center #D6DEDF;
        }

        .LoginWrapbg
        {
            background: url("product/images/loginimgbg.jpg") no-repeat scroll center center transparent;
            height: 100%;
        }

        #loginBtn
        {
            background: url("product/images/loginbtn.gif") no-repeat scroll 0 0 transparent;
            color: #FFFFFF;
            font-size: 14px;
            height: 34px;
            letter-spacing: 6px;
            line-height: 32px;
            text-align: center;
            width: 100px;
            word-spacing: 1px;
        }

        .inline-block
        {
            display: inline-block;
            overflow: hidden;
            vertical-align: middle;
        }

        .loginBar
        {
            width: 345px;
            padding-top: 60px;
            padding-left: 50px;
            font-size: 14px;
        }

        .fieldWrap
        {
            text-align: left;
            width: 300px;
            height: 30px;
            padding-top: 10px;
            overflow: hidden;
        }

        .lable
        {
            padding-top: 3px;
            width: 90px;
            float: left;
            color: #333333;
            font-size: 14px;
            text-align: right;
        }

        .inputWrap
        {
            background: url(product/images/loginInputWrapbg.gif) no-repeat left top;
            height: 28px;
            line-height: 16px;
            border: none;
            width: 200px;
            float: left;
        }

        .selectWrap
        {
            background: url(product/images/loginSelectWrapbg.gif) no-repeat left top;
        }

        .verifyCodeWrap
        {
            display: none;
        }

        .verifyCode
        {
            float: left;
        }

        .verifyCodeWrap .inputWrap
        {
        }

        .welcome
        {
            color: #2477b3;
            font-family: "微软雅黑", Tahoma, sans-serif;
            font-size: 14px;
            font-weight: bold;
            padding: 6px 0;
            padding-top: 10px;
            padding-left: 20px;
            letter-spacing: 1px;
            text-align: center;
        }

        .copyright
        {
            padding-top: 10px;
            text-align: center;
        }

        img
        {
            vertical-align: middle;
        }

        input.inputText
        {
            background: transparent none;
            border: none;
            margin-top: 3px;
            _margin-top: 1px;
            vertical-align: middle;
            color: #336699;
            border: none;
        }

        .inputWrap .inputText
        {
            width: 192px;
            height: 20px;
            font-size: 14px;
            padding-left: 6px;
            margin-top: 0px;
            padding-top: 4px;
            vertical-align: middle;
            color: #336699;
            border: none;
        }

        input.inputTextHover
        {
            background: transparent none;
            border: none;
        }

        input.inputTextFocus
        {
            background: transparent none;
            border: none;
        }

        input[type="text"]:hover, input[type="password"]:hover
        {
            background: transparent none;
            border: none;
        }

        input[type="text"]:focus, input[type="password"]:focus
        {
            background: transparent none;
            border: none;
        }

        #loginBtnWrap
        {
            padding: 12px 0px 10px 130px;
            width: 100px;
        }

        #Language_textField, #skin_textField
        {
            background: transparent none;
            color: #369;
            border: 0;
        }

        #Language_wrap .arrowimg, #skin_wrap .arrowimg
        {
            background: url(product/images/arrow_inCell.gif) no-repeat 0 0;
        }

        #loginBtn
        {
            background: url(product/images/loginbtn.gif) no-repeat 0 0;
            width: 100px;
            height: 34px;
            color: #ffffff;
            font-size: 14px;
            line-height: 32px;
            text-align: center;
            word-spacing: 1px;
            letter-spacing: 6px;
        }

        .loginLogo
        {
            width: 266px;
            height: 45px;
            margin: 0 auto;
            background: url(product/images/logo_login.png);
            _filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='product/images/logo_login.png', sizingMethod = 'scale' );
            _background: none;
        }

        html
        {
            border: 0 none;
            height: 100%;
        }

        div, p, table, th, td, font
        {
            font-family: inherit;
            font-size: 1em;
            line-height: inherit;
        }

        div, blockquote, q, iframe, form, ul, ol, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, p
        {
            margin: 0;
            padding: 0;
        }
    </style>

</head>
<body class="loginPageBody">
    <div class="LoginWrapbg">
        <div style="width: 100%; height: 50%; left: 0; top: 0; z-index: -1;"></div>
        <div
            style="width: 442px; height: 440px; margin: 0 auto; margin-top: -260px;">
            <div class="loginLogo"></div>
            <div class="welcome">欢迎登录 订单管理系统</div>
            <div class="loginBar" id="loginBar">
                <form id="form1" method="post">
                    <div class="">

                        <div class="fieldWrap">
                            <div class="lable">用户名：</div>
                            <div class="inputWrap">
                                <input name="UserName" type="text"
                                    class="inputText" id="UserName" onfocus="this.select();">
                            </div>
                        </div>

                        <div class="fieldWrap">
                            <div class="lable">密码：</div>
                            <div class="inputWrap">
                                <input name="Password" type="password"
                                    class="inputText" id="Password" onfocus="this.select();">
                            </div>
                        </div>
                        <div class="fieldWrap verifyCodeWrap" id='verifyCodeWrap'></div>



                        <div class="fieldWrap" id="loginBtnWrap">
                            <a href="#;"
                                class="inline-block" id="loginBtn" onclick="login(); return false;">登录</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="copyright">
            <div class="center">
                Copyright &copy; 2006-2013 Zving.com Inc. All rights reserved.可为科技版权所有
            </div>
        </div>
    </div>
</body>
</html>
