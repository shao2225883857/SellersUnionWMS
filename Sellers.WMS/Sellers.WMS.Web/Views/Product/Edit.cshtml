@model Sellers.WMS.Domain.ProductType
@{
    Layout = null;
}
<script type="text/javascript">
    $(function () {
        //这里写加载下拉列表、修改编辑控件的初始化值
		//parent.$.messager.progress('close');

        $('#form').form({
            url: '@Url.Action("Edit", "Product")',
            onSubmit: function () {
                parent.$.messager.progress({
                    title: '提示',
                    text: '数据处理中，请稍后....'
                });
                var isValid = $(this).form('validate');
                if (!isValid) {
                    parent.$.messager.progress('close');
                }
                return isValid;
            },
            success: function (result) {
                parent.$.messager.progress('close');
                result = $.parseJSON(result);
                if (result.IsSuccess) {
                    //parent.$.modalDialog.openner_treeGrid.treegrid('reload');//之所以能在这里调用到parent.$.modalDialog.openner_treeGrid这个对象，是因为resource.jsp页面预定义好了
                    //parent.layout_west_tree.tree('reload');
                    parent.$.modalDialog.penner_grid.datagrid('reload');
                    parent.$.modalDialog.handler.dialog('close');
                }
                else {
                    alert(result.Result);
                }
            }
        });
    });
</script>
<div class="easyui-layout" data-options="fit:true,border:false">
    <div data-options="region:'center',border:false" title="" style="overflow: hidden;">
        @using (Html.BeginForm("", "", FormMethod.Post, new { id = "form" }))
        {
            <table class="grid">
                <tr>
                    <td align="right">主键：</td>
                    <td align="left"><input  type='text' name='Id' id='Id' value='@Model.Id' ></input></td>
                    <td align="right">商品编号：</td>
                    <td align="left"><input  type='text' name='SKU' id='SKU' value='@Model.SKU' ></input></td>
                </tr>
                <tr>
                    <td align="right">商品名称：</td>
                    <td align="left"><input  type='text' name='Title' id='Title' value='@Model.Title' ></input></td>
                    <td align="right">商品状态：</td>
                    <td align="left"><input  type='text' name='Status' id='Status' value='@Model.Status' ></input></td>
                </tr>
                <tr>
                    <td align="right">类别：</td>
                    <td align="left"><input  type='text' name='Category' id='Category' value='@Model.Category' ></input></td>
                    <td align="right">型号：</td>
                    <td align="left"><input  type='text' name='Model' id='Model' value='@Model.Model' ></input></td>
                </tr>
                <tr>
                    <td align="right">品牌：</td>
                    <td align="left"><input  type='text' name='Brand' id='Brand' value='@Model.Brand' ></input></td>
                    <td align="right">规格：</td>
                    <td align="left"><input  type='text' name='Standard' id='Standard' value='@Model.Standard' ></input></td>
                </tr>
                <tr>
                    <td align="right">商品价格：</td>
                    <td align="left"><input  type='text' name='Price' id='Price' value='@Model.Price' ></input></td>
                    <td align="right">重量：</td>
                    <td align="left"><input  type='text' name='Weight' id='Weight' value='@Model.Weight' ></input></td>
                </tr>
                <tr>
                    <td align="right">长：</td>
                    <td align="left"><input  type='text' name='Long' id='Long' value='@Model.Long' ></input></td>
                    <td align="right">宽：</td>
                    <td align="left"><input  type='text' name='Wide' id='Wide' value='@Model.Wide' ></input></td>
                </tr>
                <tr>
                    <td align="right">高：</td>
                    <td align="left"><input  type='text' name='High' id='High' value='@Model.High' ></input></td>
                    <td align="right">库存天数：</td>
                    <td align="left"><input  type='text' name='DayByStock' id='DayByStock' value='@Model.DayByStock' ></input></td>
                </tr>
                <tr>
                    <td align="right">简单描述：</td>
                    <td align="left"><input  type='text' name='Summary' id='Summary' value='@Model.Summary' ></input></td>
                    <td align="right">包装注意事项：</td>
                    <td align="left"><input  type='text' name='PackMemo' id='PackMemo' value='@Model.PackMemo' ></input></td>
                </tr>
                <tr>
                    <td align="right">是否侵权：</td>
                    <td align="left"><input  type='text' name='IsInfraction' id='IsInfraction' value='@Model.IsInfraction' ></input></td>
                    <td align="right">大图片网址：</td>
                    <td align="left"><input  type='text' name='PicUrl' id='PicUrl' value='@Model.PicUrl' ></input></td>
                </tr>
                <tr>
                    <td align="right">小图片网址：</td>
                    <td align="left"><input  type='text' name='SPicUrl' id='SPicUrl' value='@Model.SPicUrl' ></input></td>
                    <td align="right">采购人：</td>
                    <td align="left"><input  type='text' name='Purchaser' id='Purchaser' value='@Model.Purchaser' ></input></td>
                </tr>
                <tr>
                    <td align="right">验货人：</td>
                    <td align="left"><input  type='text' name='Examiner' id='Examiner' value='@Model.Examiner' ></input></td>
                    <td align="right">包装人：</td>
                    <td align="left"><input  type='text' name='Packer' id='Packer' value='@Model.Packer' ></input></td>
                </tr>
                <tr>
                    <td align="right">包装系数：</td>
                    <td align="left"><input  type='text' name='PackCoefficient' id='PackCoefficient' value='@Model.PackCoefficient' ></input></td>
                    <td align="right">电子：</td>
                    <td align="left"><input  type='text' name='IsElectronic' id='IsElectronic' value='@Model.IsElectronic' ></input></td>
                </tr>
                <tr>
                    <td align="right">电池：</td>
                    <td align="left"><input  type='text' name='HasBattery' id='HasBattery' value='@Model.HasBattery' ></input></td>
                    <td align="right">库位：</td>
                    <td align="left"><input  type='text' name='Location' id='Location' value='@Model.Location' ></input></td>
                </tr>
                <tr>
                    <td align="right">创建时间：</td>
                    <td align="left"><input  type='text' name='CreateOn' id='CreateOn' value='@Model.CreateOn' ></input></td>
                    <td align="right">创建人：</td>
                    <td align="left"><input  type='text' name='CreateBy' id='CreateBy' value='@Model.CreateBy' ></input></td>
                </tr>

			</table>
        }
    </div>
</div>
