@model Sellers.WMS.Domain.OrderProductType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Sellers.WMS.Domain.OrderProductType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Sellers.WMS.Domain.OrderProductType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">订单ID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.OId)
    </div>

    <div class="display-label">订单号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.OrderNo)
    </div>

    <div class="display-label">外部商品SKU</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ExSKU)
    </div>

    <div class="display-label">名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Title)
    </div>

    <div class="display-label">数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Qty)
    </div>

    <div class="display-label">对应内部库存SKU</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SKU)
    </div>

    <div class="display-label">产品描述</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Remark)
    </div>

    <div class="display-label">产品规格</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Standard)
    </div>

    <div class="display-label">产品价格</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Price)
    </div>

    <div class="display-label">产品网址</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Url)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
