                    <td align="left"><input  type='text' name='CId' id='CId' value='@Model.CId' ></input></td>
                    <td align="right">公司：</td>
                    <td align="left"><input  type='text' name='CompanyName' id='CompanyName' value='@Model.CompanyName' ></input></td>
                </tr>
                <tr>
                    <td align="right">部门主键：</td>
                    <td align="left"><input  type='text' name='DId' id='DId' value='@Model.DId' ></input></td>
                    <td align="right">部门名称：</td>
                    <td align="left"><input  type='text' name='DepartmentName' id='DepartmentName' value='@Model.DepartmentName' ></input></td>
                </tr>
                <tr>
                    <td align="right">工作组主键：</td>
                    <td align="left"><input  type='text' name='WorkgroupId' id='WorkgroupId' value='@Model.WorkgroupId' ></input></td>
                    <td align="right">工作组名称：</td>
                    <td align="left"><input  type='text' name='WorkgroupName' id='WorkgroupName' value='@Model.WorkgroupName' ></input></td>
                </tr>
                <tr>
                    <td align="right">性别：</td>
                    <td align="left"><input  type='text' name='Gender' id='Gender' value='@Model.Gender' ></input></td>
                    <td align="right">电话：</td>
                    <td align="left"><input  type='text' name='Telephone' id='Telephone' value='@Model.Telephone' ></input></td>
                </tr>
                <tr>
                    <td align="right">手机：</td>
                    <td align="left"><input  type='text' name='Mobile' id='Mobile' value='@Model.Mobile' ></input></td>
                    <td align="right">出生日期：</td>
                    <td align="left"><input  type='text' name='Birthday' id='Birthday' value='@Model.Birthday' ></input></td>
                </tr>
                <tr>
                    <td align="right">岗位：</td>
                    <td align="left"><input  type='text' name='Duty' id='Duty' value='@Model.Duty' ></input></td>
                    <td align="right">电子邮件：</td>
                    <td align="left"><input  type='text' name='Email' id='Email' value='@Model.Email' ></input></td>
                </tr>
                <tr>
                    <td align="right">家庭住址：</td>
                    <td align="left"><input  type='text' name='HomeAddress' id='HomeAddress' value='@Model.HomeAddress' ></input></td>
                    <td align="right">最后访问时间：</td>
                    <td align="left"><input  type='text' name='LastVisit' id='LastVisit' value='@Model.LastVisit' ></input></td>
                </tr>
                <tr>
                    <td align="right">审核状态：</td>
                    <td align="left"><input  type='text' name='AuditStatus' id='AuditStatus' value='@Model.AuditStatus' ></input></td>
                    <td align="right">删除标志：</td>
                    <td align="left"><input  type='text' name='DeletionStateCode' id='DeletionStateCode' value='@Model.DeletionStateCode' ></input></td>
                </tr>
                <tr>
                    <td align="right">排序码：</td>
                    <td align="left"><input  type='text' name='SortCode' id='SortCode' value='@Model.SortCode' ></input></td>
                    <td align="right">备注：</td>
