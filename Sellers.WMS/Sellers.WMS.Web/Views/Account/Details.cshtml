@model Sellers.WMS.Domain.AccountType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Sellers.WMS.Domain.AccountType</title>
</head>
<body>
<fieldset>
    <legend>Sellers.WMS.Domain.AccountType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">账户名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AccountName)
    </div>

    <div class="display-label">平台网址</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AccountUrl)
    </div>

    <div class="display-label">APIKey</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ApiKey)
    </div>

    <div class="display-label">API密钥</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ApiSecret)
    </div>

    <div class="display-label">API会话</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ApiToken)
    </div>

    <div class="display-label">平台</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Platform)
    </div>

    <div class="display-label">状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Status)
    </div>

    <div class="display-label">描述</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>

    <div class="display-label">负责人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Manager)
    </div>


</fieldset>
<p>
    @Html.ActionLink("编辑", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("返回列表", "Index")
</p>
</body>
</html>
