@model Sellers.WMS.Domain.OrderBuyerType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Sellers.WMS.Domain.OrderBuyerType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Sellers.WMS.Domain.OrderBuyerType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">客户名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerName)
    </div>

    <div class="display-label">客户邮件</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerEmail)
    </div>

    <div class="display-label">客户购买次数</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyCount)
    </div>

    <div class="display-label">客户购买金额</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyAmount)
    </div>

    <div class="display-label">第一次购买时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FristBuyOn)
    </div>

    <div class="display-label">最后一次购买时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LastBuyOn)
    </div>

    <div class="display-label">客户备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Remark)
    </div>

    <div class="display-label">客户类型</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerType)
    </div>

    <div class="display-label">平台</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Platform)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
